def calculate(arg):
    z=str(arg)
    u=0
    l=0
    for i in z:
        if i.isupper():
            u+=1
        elif i.islower():
            l+=1
        else:
            pass
    print('No. of Upper case characters :',u)
    print('No. of Lower case characters :',l)
a='The quick Brow Fox'
calculate(a)
